{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "auto_update": true,
  "browser_automation_engine": {
    "provider": "playwright"
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "notification": {
    "force_enable": true
  },
  "lsp": {},
  "sisyphus_agent": {
    "disabled": false,
    "default_builder_enabled": true,
    "planner_enabled": true,
    "replace_plan": false
  },
  "experimental": {
    "aggressive_truncation": false,
    "auto_resume": true,
    "truncate_all_tool_outputs": false,
    "dynamic_context_pruning": {
      "enabled": true,
      "notification": "detailed",
      "turn_protection": {
        "enabled": true,
        "turns": 3
      },
      "strategies": {
        "deduplication": {
          "enabled": true
        },
        "supersede_writes": {
          "enabled": true,
          "aggressive": false
        },
        "purge_errors": {
          "enabled": true,
          "turns": 5
        }
      }
    }
  },
  "ralph_loop": {
    "enabled": false,
    "default_max_iterations": 100
  },
  "background_task": {
    "defaultConcurrency": 4,
    "staleTimeoutMs": 300000
  },
  "disabled_hooks": [
    "anthropic-context-window-limit-recovery"
  ],
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max"
    },
    "sisyphus-junior": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "low"
    },
    "build": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
    },
    "plan": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max"
    },
    "OpenCode-Builder": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
    },
    "hephaestus": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
    },
    "oracle": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash",
      "variant": "high"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "prometheus": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max"
    },
    "metis": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
    },
    "momus": {
      "model": "google/antigravity-claude-sonnet-4-5"
    },
    "atlas": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "executor": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "low"
    },
    "reviewer": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
    },
    "tester": {
      "model": "google/antigravity-claude-sonnet-4-5-thinking",
      "variant": "max"
