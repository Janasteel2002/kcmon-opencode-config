      "variant": "max"
    },
    "doc-writer": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    }
  },
  "categories": {
    "ultrabrain": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
      "variant": "max",
      "reasoningEffort": "xhigh"
    },
    "security": {
      "model": "google/antigravity-claude-opus-4-6-thinking",
